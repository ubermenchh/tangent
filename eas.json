{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "GRADLE_OPTS": "-Dorg.gradle.daemon=true -Dorg.gradle.parallel=true -Dorg.gradle.caching=true"
      },
      "cache": {
        "key": "dev-v1",
        "paths": ["~/.gradle/caches"]
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {}
  }
}