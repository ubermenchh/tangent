{
  "expo": {
    "name": "tangent",
    "slug": "tangent",
    "scheme": "tangent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tangent.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/tangent.png",
      "resizeMode": "contain",
      "backgroundColor": "#111113"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tangent.png",
        "backgroundColor": "#111113"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/tangent.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ]
  }
}
