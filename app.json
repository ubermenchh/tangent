{
  "expo": {
    "name": "tangent",
    "slug": "tangent",
    "scheme": "tangent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tangent.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/tangent.png",
      "resizeMode": "contain",
      "backgroundColor": "#111113"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tangent.png",
        "backgroundColor": "#111113"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.SEND_SMS",
        "android.permission.READ_SMS",
        "android.permission.RECEIVE_SMS",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_NOTIFICATION_POLICY",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.ACTIVITY_RECOGNITION",
        "android.permission.BODY_SENSORS",
        "android.permission.VIBRATE",
        "android.permission.QUERY_ALL_PACKAGES"
      ],
      "package": "com.ubermenchh.tangent"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/tangent.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Tangent to access your contacts."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "42843de8-5c21-4b5a-bcf0-fa37fadabcc4"
      }
    }
  }
}
