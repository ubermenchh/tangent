{
  "expo": {
    "name": "tangent",
    "slug": "tangent",
    "scheme": "tangent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tangent.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/tangent.png",
      "resizeMode": "contain",
      "backgroundColor": "#111113"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tangent.png",
        "backgroundColor": "#111113"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.SEND_SMS",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "package": "com.ubermenchh.tangent"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/tangent.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Tangent to access your contacts."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "42843de8-5c21-4b5a-bcf0-fa37fadabcc4"
      }
    }
  }
}
